							};
							c13 {
								regulator-consumer-supply = "vdd";
								regulator-consumer-device = "stm8t143.2";
							};
							c14 {
								regulator-consumer-supply = "hvdd_sata";
								regulator-consumer-device = "tegra-sata.0";
							};
							c15 {
								regulator-consumer-supply = "vdd";
