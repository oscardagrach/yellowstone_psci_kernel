								regulator-consumer-device = "sdhci-tegra.3";
							};
							c9 {
								regulator-consumer-supply = "hvdd_pex";
								regulator-consumer-device = "tegra-pcie";
							};
							c10 {
								regulator-consumer-supply = "hvdd_pex_pll_e";
								regulator-consumer-device = "tegra-pcie";
							};
							c11 {
								regulator-consumer-supply = "vddio_pex_ctl";
