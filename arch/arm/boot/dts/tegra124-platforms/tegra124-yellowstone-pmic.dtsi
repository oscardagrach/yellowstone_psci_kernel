								regulator-consumer-supply = "avdd_usb";
								regulator-consumer-device = "tegra-ehci.1";
							};
							c6 {
								regulator-consumer-supply = "avdd_usb";
								regulator-consumer-device = "tegra-ehci.2";
							};
							c7 {
								regulator-consumer-supply = "hvdd_usb";
								regulator-consumer-device = "tegra-xhci";
							};
							c8 {
